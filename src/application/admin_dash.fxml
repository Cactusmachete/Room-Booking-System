<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="574.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.adminController">
   <children>
      <TabPane layoutY="-4.0" prefHeight="574.0" prefWidth="816.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Booked Rooms">
               <content>
                  <AnchorPane>
                     <children>
                        <AnchorPane prefHeight="372.0" prefWidth="673.0">
                           <children>
                              <TableView fx:id="bookedTable" prefHeight="536.0" prefWidth="816.0">
                              <placeholder>
        						<Label text="There are no booked rooms!" />
    						  </placeholder>
                                <columns>
                                  <TableColumn fx:id="bookedRoom" editable="false" prefWidth="130.0" style="-fx-alignment: CENTER;" text="Room Booked" />
                                  <TableColumn fx:id="bookedPurpose" editable="false" prefWidth="336.0" style="-fx-alignment: CENTER;" text="Purpose of Booking" />
                                  <TableColumn fx:id="bookedCap" editable="false" prefWidth="115.0" style="-fx-alignment: CENTER;" text="Capacity" />
                                  <TableColumn fx:id="bookedSlot" editable="false" prefWidth="134.0" style="-fx-alignment: CENTER;" text="Slot" />
                                  <TableColumn fx:id="bookedCancel" editable="false" prefWidth="100.0" style="-fx-alignment: CENTER;" />
                                </columns>
                              </TableView>
                           </children>
                        </AnchorPane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
          <Tab text="Available Rooms">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <AnchorPane prefHeight="374.0" prefWidth="673.0">
                         <children>
                              <TableView fx:id="availTable" layoutY="50.0" prefHeight="485.0" prefWidth="816.0">
                              	<placeholder>
        							<Label text="There are no available rooms!" />
    						  	</placeholder>
                                <columns>
                                  <TableColumn fx:id="availRoomNum" editable="false" prefWidth="155.0" style="-fx-alignment: CENTER;" text="Room Number" />
                                  <TableColumn fx:id="availCap" editable="false" prefWidth="147.0" style="-fx-alignment: CENTER;" text="Capacity" />
                                  <TableColumn fx:id="availSlot" editable="false" prefWidth="389.0" style="-fx-alignment: CENTER;" text="Available Slots" />
                                    <TableColumn fx:id="availBook" editable="false" prefWidth="124.0" style="-fx-alignment: CENTER;" />
                                </columns>
                              </TableView>
                              <Button fx:id="availGo" layoutX="475.0" layoutY="11.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="55.0" text="Go" />
                              <TextField fx:id="availDate" layoutX="254.0" layoutY="11.0" promptText="dd/mm/yyyy" />
                              <Label layoutX="17.0" layoutY="16.0" prefHeight="21.0" prefWidth="237.0" text="View Available Rooms for:    Date" />
                           </children>
                          </AnchorPane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Booking Requests">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <AnchorPane prefHeight="368.0" prefWidth="673.0">
                           <children>
                              <TableView prefHeight="535.0" prefWidth="816.0">
                              	<placeholder>
        							<Label text="There are no booking requests!" />
    						  	</placeholder>
                                <columns>
                                  <TableColumn editable="false" prefWidth="139.0" style="-fx-alignment: CENTER;" text="Preferred Room" />
                                  <TableColumn editable="false" prefWidth="328.0" style="-fx-alignment: CENTER;" text="Purpose of Booking" />
                                  <TableColumn editable="false" prefWidth="74.0" style="-fx-alignment: CENTER;" text="Capacity" />
                                  <TableColumn editable="false" minWidth="0.0" prefWidth="126.0" style="-fx-alignment: CENTER;" text="Slot" />
                                  <TableColumn editable="false" prefWidth="148.0" style="-fx-alignment: CENTER;" text="Accept/Reject" />
								</columns>
                              </TableView>
                           </children>
                         </AnchorPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>

        </tabs>
      </TabPane>
      <Button layoutX="737.0" layoutY="4.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="76.0" text="Log out" />
   </children>
</AnchorPane>
